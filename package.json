{
  "name": "ruscello",
  "version": "0.1.0",
  "description": "a TypeScript Flux implementation",
  "scripts": {
    "start": "make -j",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
