    "browserify": "^9.0.3",
    "cheerio": "^0.18.0",
    "date-utils": "^1.2.16",
    "eventemitter2": "^0.4.14",
    "express": "^4.10.6",
    "flux": "^2.0.1",
    "jest-cli": "^0.1.18",
    "keymirror": "^0.1.0",
    "react": "^0.13.1",
    "react-hot-loader": "^1.2.8",
